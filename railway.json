{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "echo 'No build needed'"
  },
  "deploy": {
    "startCommand": "python3 -m http.server 8000 --bind 0.0.0.0",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "port": 8000
  }
}
